    <model name="Blimp">
      <pose>1.5 0 2 0 0 -1.57</pose>
      <static>false</static>
      <link name="body">
      
        <collision name='collision'>
          <geometry>
            <mesh>
             <uri>model://blimp_envelope_final/meshes/blimp_envelope_final.dae</uri>
            </mesh>
          </geometry> 
        </collision>     

        <visual name="visual">
          <geometry>
            <mesh><uri>model://blimp_envelope_final/meshes/blimp_envelope_final.dae</uri></mesh>
          </geometry>
        </visual>
      </link>
      <!-- AR marker as link on the blimp -->
      <link name= "AR_Marker">
        <pose>0 -0.5 0.5 1.57 0 0</pose>
        
          <collision name='collision'>
            <geometry>
              <mesh>
              <uri>model://AR_marker72/meshes/ar72_2.dae</uri>
              </mesh>
            </geometry> 
          </collision>     

          <visual name="visual">
            <geometry>
              <mesh><uri>model://AR_marker72/meshes/ar72_2.dae</uri></mesh>
            </geometry>
          </visual>
      </link>
<!-- Link's position is with respect to the model position (model's position becomes origin) -->
      <link name="Fake_link">
        <pose>0 0 -1 0 0 0</pose>
        <collision name="fake_collision">
          <geometry>
            <box>
              <size>0.5 0.5 2</size>
            </box>
          </geometry>
        </collision>
        <!-- Hiding visual so it appears to be flying -->
        <visual name="fake_visual">
          <geometry>
            <box>
              <size>0.5 0.5 2</size>
            </box>
          </geometry>
        </visual>
      </link>

    <!-- fixed joint constraining the blimp to the fake link -->

    <joint name="Joint1" type="fixed">
        <pose>1.5 0 1 0 0 0</pose>
        <child>body</child>
        <parent>Fake_link</parent>
    </joint>

  <!-- fixed joint constraining the AR marker to the blimp -->
    <joint name="Joint2" type="fixed">
        <pose>0.5 0 2.5 0 0 0</pose>
        <child>AR_Marker</child>
        <parent>body</parent>
    </joint>

    <!-- prismatic joint between the fake link and the ground so that it only translates -->
        

